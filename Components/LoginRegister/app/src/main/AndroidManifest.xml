        <activity android:name=".FoodCravings"
            android:theme="@style/Theme"/>
        <activity android:name=".ExerciseCalculator"
            android:theme="@style/Theme"
            android:screenOrientation="portrait"/>
        <activity android:name=".Advanced_Survey" />
        <activity android:name=".Second" />
        <activity android:name=".CustomDiet" />
        <activity android:name=".Update_wtht"
            android:theme="@style/Theme"/>
        <activity android:name=".Piechart_Calories" />
        <!-- <activity android:name=".Dates" /> -->
        <activity android:name=".Graphbyleo" />
        <activity
            android:name=".Water_graph"
            android:theme="@style/Theme" />
        <activity android:name=".Water_graph2" />

        <receiver android:name=".NotificationReciever" />

        <activity android:name=".Notification" />
        <activity android:name=".CalorieGraph"></activity>
    </application>

</manifest>